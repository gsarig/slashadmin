msgstr "Φωτεινό"

#: options.php:487
msgid "None (I will style it my self)"
msgstr "Κανένα (θα το μορφοποιήσω μόνος μου)"

#: options.php:492
msgid "Position"
msgstr "Θέση"

#: options.php:493
msgid "Select the position of the message box."
msgstr "Επιλέξτε τη θέση του μηνύματος."

#: options.php:497
msgid "Bottom"
msgstr "Κάτω"

#: options.php:498
msgid "Top"
msgstr "Επάνω"

#: options.php:499
